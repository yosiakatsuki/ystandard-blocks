{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
	"apiVersion": 3,
	"name": "ystdb/svg-icon",
	"title": "アイコン",
	"description": "アイコン表示ブロック",
	"version": "1.0.0",
	"keywords": [
		"icon"
	],
	"attributes": {
		"icon": {
			"type": "string",
			"default": "info"
		},
		"align": {
			"type": "string",
			"default": "center"
		},
		"textColor": {
			"type": "string"
		},
		"customTextColor": {
			"type": "string"
		},
		"fontSize": {
			"type": "string"
		},
		"customFontSize": {
			"type": "string",
			"default": "24px"
		},
		"responsiveFontSize": {
			"type": "object"
		},
		"url": {
			"type": "string"
		},
		"linkTarget": {
			"type": "string"
		},
		"rel": {
			"type": "string"
		},
		"padding": {
			"type": "object"
		},
		"responsivePadding": {
			"type": "object"
		},
		"width": {
			"type": "string"
		},
		"responsiveWidth": {
			"type": "object"
		},
		"height": {
			"type": "string"
		},
		"responsiveHeight": {
			"type": "object"
		},
		"iconSize": {
			"type": "string",
			"default": "5"
		}
	},
	"supports": {
		"className": false,
		"align": false
	},
	"textdomain": "ystandard-blocks",
	"editorScript": "file:./index.tsx",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
