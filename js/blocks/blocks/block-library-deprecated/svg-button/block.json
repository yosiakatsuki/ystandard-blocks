{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "ystdb/svg-button",
  "title": "カスタムボタン",
  "description": "yStandard Blocks カスタムボタン",
  "version": "2.0.0",
  "keywords": [
    "btn",
    "button",
    "ボタン",
    "アイコン"
  ],
  "attributes": {
    "hideSp": {
      "type": "boolean",
      "default": false
    },
    "hideMd": {
      "type": "boolean",
      "default": false
    },
    "hideLg": {
      "type": "boolean",
      "default": false
    },
    "taxFilterType": {
      "type": "string",
      "default": "only"
    },
    "taxonomy": {
      "type": "string"
    },
    "terms": {
      "type": "array"
    },
    "applyChildren": {
      "type": "boolean",
      "default": false
    },
    "hideAMP": {
      "type": "boolean",
      "default": false
    },
    "onlyAMP": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "className": false,
    "html": false,
    "align": false,
    "ystdtdHiddenBySize": false
  },
  "styles": [
    {
      "name": "fill",
      "label": "塗りつぶし",
      "isDefault": true
    },
    {
      "name": "outline",
      "label": "アウトライン"
    }
  ],
  "textdomain": "ystandard-toolbox",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}