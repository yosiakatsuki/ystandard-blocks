{
  "name": "ystandard-blocks",
  "version": "0.0.1",
  "description": "yStandard用ブロック追加プラグイン",
  "scripts": {
    "lint-all": "npm run lint-js:fix && npm run csscomb:all && npm run lint-css-fix",
    "lint-js": "wp-scripts lint-js blocks ./src/js",
    "lint-js:fix": "npm run lint-js -- --fix",
    "lint-css:src": "wp-scripts lint-style 'src/**/*.scss'",
    "lint-css:src:fix": "npm run lint-css:src -- --fix",
    "lint-css:blocks": "wp-scripts lint-style 'blocks/**/*.scss'",
    "lint-css:blocks:fix": "npm run lint-css:blocks -- --fix",
    "lint-css-fix": "npm run lint-css:src:fix && npm run lint-css:blocks:fix",
    "csscomb:src": "csscomb src",
    "csscomb:blocks": "csscomb blocks",
    "csscomb:all": "npm run csscomb:blocks && npm run csscomb:src",
    "start:block": "wp-scripts start --config ystandard-blocks-webpack-config.js",
    "build:block": "wp-scripts build --config ystandard-blocks-webpack-config.js",
    "build:block-app": "gulp js:block-app",
    "build:icon": "node src/js/util/create-icon-list.js",
    "build:css": "gulp sass",
    "build": "npm run build:icon && npm run build:block && gulp sass",
    "watch": "npm run start:block & gulp watch",
    "zip": "gulp zip",
    "build:deploy-files": "gulp createDeployFiles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yosiakatsuki/ystandard-blocks.git"
  },
  "author": "",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/yosiakatsuki/ystandard-blocks/issues"
  },
  "homepage": "https://github.com/yosiakatsuki/ystandard-blocks#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@wordpress/browserslist-config": "^2.6.0",
    "@wordpress/scripts": "7.1.2",
    "autoprefixer": "^9.7.4",
    "browserslist": "^4.8.6",
    "classnames": "^2.2.6",
    "css-loader": "^3.4.2",
    "css-mqpacker": "^7.0.0",
    "csscomb": "^4.3.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "file-loader": "^4.3.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-zip": "^5.0.1",
    "lodash": "^4.17.15",
    "style-loader": "^1.1.3",
    "watch": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "@fonticonpicker/react-fonticonpicker": "^1.2.0",
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.3.0"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
