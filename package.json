{
  "name": "ystandard-blocks",
  "version": "0.0.1",
  "description": "yStandard用ブロック追加プラグイン",
  "scripts": {
    "activate:theme": "wp-env run cli 'wp theme activate ystandard'",
    "start:update": "wp-env start --xdebug=debug,develop -- --update && npm run activate:theme && npm run import",
    "start": "wp-env start --xdebug=debug,develop && npm run import && open http://localhost:10010/",
    "stop": "npm run export && wp-env stop",
    "lint:js": "wp-scripts lint-js blocks ./src/js --fix",
    "lint:css:src": "wp-scripts lint-style 'src/**/*.scss' --fix",
    "lint:css:blocks": "wp-scripts lint-style 'blocks/**/*.scss' --fix",
    "lint:css": "run-s lint:css:*",
    "lint:php": "composer phpcs",
    "lint": "run-s lint:*",
    "build:css:postcss": "postcss css/**/*.css -r",
    "build:css:sass": "sass src/sass:css blocks:css/blocks --no-source-map",
    "build:css": "run-s build:css:sass build:css:postcss",
    "build:block": "wp-scripts build --config webpack.blocks.config.js",
    "build:icons": "node src/js/script/create-svg-icon-list.js",
    "build:admin": "wp-scripts build --config webpack.admin-menu.config.js && cp -f ./src/js/admin-menu/inline-style/schema.json ./assets/admin-menu/inline-style",
    "build:app": "wp-scripts build --config webpack.app.config.js",
    "build": "run-s build:*",
    "watch:css:postcss": "postcss temp/css/**/*.css --base temp/css --dir css --verbose -w",
    "watch:css:sass": "sass src/sass:temp/css ./blocks:temp/css/blocks --no-source-map --no-stop-on-error -w",
    "watch:css": "run-p watch:css:*",
    "watch:js:block": "wp-scripts start --config webpack.blocks.config.js",
    "watch:js:admin": "wp-scripts start --config webpack.admin-menu.config.js",
    "watch:js:app": "wp-scripts start --config webpack.app.config.js",
    "watch:js": "run-p watch:js:*",
    "watch": "run-p watch:*",
    "format": "wp-scripts format",
    "i18n:pot": "wp i18n make-pot --domain=\"ystandard-blocks\" --exclude=\"library\" ./ languages/ystandard-blocks.pot",
    "i18n:po2json:blocks": "po2json languages/ystandard-blocks-ja.po languages/ystandard-blocks-ja-ystandard-blocks-editor.json -f jed -p",
    "clean:temp": "rimraf temp",
    "clean:upload": "rimraf upload & rimraf ystandard-blocks",
    "clean:css": "rimraf css",
    "clean:js": "rimraf js",
    "clean:zip": "rimraf ystandard-blocks.zip",
    "clean": "run-p clean:*",
    "fixtures:clean": "rimraf \"test/integration/fixtures/blocks/*.+(json|serialized.html)\"",
    "fixtures:generate": "cross-env GENERATE_MISSING_FIXTURES=y npm run test-unit test/integration/full-content/ && npm run format test/integration/fixtures/blocks/*.json",
    "fixtures:regenerate": "npm run fixtures:clean && npm run fixtures:generate",
    "fixtures": "npm run fixtures:regenerate test/integration/full-content/full-content.test.js",
    "test-unit": "wp-scripts test-unit-js --config test/unit/jest.config.js",
    "test-unit-php": "wp-env start && wp-env run phpunit \"phpunit -c /var/www/html/wp-content/plugins/ystandard-blocks/phpunit.xml.dist --verbose\"",
    "test": "npm run test-unit && npm run test-unit-php",
    "zip:copy:base": "rsync -a --exclude='.*' --exclude='/node_modules' --exclude='/vendor' --exclude='/bin' --exclude='/upload' --exclude='/composer.*' --exclude='*.config.js' --exclude='*.config.json' --exclude='*.xml.dist' --exclude='/phpunit' --exclude='/test' --exclude='/src' --exclude='package*.json' --exclude='/blocks' --exclude='/temp' --exclude='ystandard-blocks*.json' . ystandard-blocks",
    "zip:copy:blocks": "rsync -am --exclude='*.scss' --exclude='*.js' --exclude='test/' --exclude='deprecated/' ./blocks ystandard-blocks/",
    "zip:archive": "zip -9 -qmr ystandard-blocks.zip ystandard-blocks",
    "zip": "run-s clean:upload clean:zip zip:copy:base zip:copy:blocks zip:archive",
    "mkdir:upload": "mkdir upload",
    "copy:json:production": "cp ystandard-blocks.json upload/ystandard-blocks.json",
    "copy:json:develop": "cp ystandard-blocks-beta.json upload/ystandard-blocks.json",
    "copy:plugin": "cp ystandard-blocks.zip upload/ystandard-blocks.zip",
    "copy:production": "run-s mkdir:upload copy:json:production copy:plugin",
    "copy:develop": "run-s mkdir:upload copy:json:develop copy:plugin",
    "update:wp-scripts-packages": "wp-scripts packages-update",
    "update": "run-p update:*",
    "cli": "wp-env run cli",
    "wp": "wp-env run cli wp",
    "export:uploads": "npm run cli 'cp -r wp-content/uploads .wp-content'",
    "export:db": "npm run wp 'db export .wp-content/db.sql'",
    "export:clean:remove": "npm run cli 'rm -rf .wp-content/uploads'",
    "export:clean:mkdir": "npm run cli 'mkdir -p .wp-content/uploads'",
    "export:clean": "run-s export:clean:remove export:clean:mkdir",
    "export": "npm run export:clean && run-p export:uploads export:db",
    "import:uploads": "npm run cli 'cp -r .wp-content/uploads wp-content'",
    "import:db": "npm run wp 'db import .wp-content/db.sql'",
    "import": "run-s import:*",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yosiakatsuki/ystandard-blocks.git"
  },
  "author": "",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/yosiakatsuki/ystandard-blocks/issues"
  },
  "homepage": "https://github.com/yosiakatsuki/ystandard-blocks#readme",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@emotion/babel-plugin": "^11.9.2",
    "@wordpress/block-editor": "^8.6.0",
    "@wordpress/block-serialization-default-parser": "^4.7.0",
    "@wordpress/blocks": "^11.6.0",
    "@wordpress/browserslist-config": "^4.1.2",
    "@wordpress/env": "^4.5.0",
    "@wordpress/icons": "^8.2.0",
    "@wordpress/prettier-config": "^1.2.0",
    "@wordpress/scripts": "^22.4.0",
    "autoprefixer": "^10.4.4",
    "babel-plugin-inline-json-import": "^0.3.2",
    "browserslist": "^4.20.2",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "css-declaration-sorter": "^6.2.2",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.7",
    "deepmerge": "^4.2.2",
    "eslint": "^8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-glob": "^3.2.11",
    "feather-icons": "^4.29.0",
    "fibers": "^5.0.1",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "po2json": "^1.0.0-beta-3",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "prettier": "npm:wp-prettier@2.0.5",
    "react-feather": "^2.0.9",
    "react-toastify": "^8.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "sass-loader": "^12.6.0",
    "simple-icons": "^6.19.0",
    "style-loader": "^3.3.1"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
