{
  "name": "ystandard-blocks",
  "version": "0.0.1",
  "description": "yStandard用ブロック追加プラグイン",
  "scripts": {
    "start": "wp-scripts start --config ystandard-blocks-webpack-config.js",
    "build:block": "wp-scripts build --config ystandard-blocks-webpack-config.js",
    "build:sass": "node-sass src/sass -o src/css",
    "build:postcss": "postcss src/css/*.css -u autoprefixer postcss-clean css-mqpacker --dir css --no-map",
    "build:css": "npm run build:sass && npm run build:postcss",
    "build:icon": "node util/create-icon-list.js",
    "watch:css": "watch 'npm run build:css' src/sass",
    "watch:css-block": "watch 'npm run build:css' block/",
    "build": "npm run build:block & npm run build:css & npm run build:icon",
    "watch": "npm run start & npm run watch:css && watch:css-block"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yosiakatsuki/ystandard-blocks.git"
  },
  "author": "",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/yosiakatsuki/ystandard-blocks/issues"
  },
  "homepage": "https://github.com/yosiakatsuki/ystandard-blocks#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-brands-svg-icons": "^5.10.1",
    "@fortawesome/free-regular-svg-icons": "^5.10.1",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@wordpress/scripts": "3.4.0",
    "autoprefixer": "^9.6.1",
    "classnames": "^2.2.6",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "file-loader": "^4.2.0",
    "node-sass": "^4.12.0",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^6.1.3",
    "style-loader": "^1.0.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@fonticonpicker/react-fonticonpicker": "^1.2.0",
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.3.0"
  }
}
