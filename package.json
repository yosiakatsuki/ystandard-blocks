{
  "name": "ystandard-blocks",
  "version": "0.0.1",
  "description": "yStandard用ブロック追加プラグイン",
  "scripts": {
    "lint-all": "npm run lint-js:fix && npm run lint-css-fix",
    "lint-js": "wp-scripts lint-js blocks ./src/js",
    "lint-js:fix": "npm run lint-js -- --fix",
    "lint-css:src": "wp-scripts lint-style 'src/**/*.scss'",
    "lint-css:src:fix": "npm run lint-css:src -- --fix",
    "lint-css:blocks": "wp-scripts lint-style 'blocks/**/*.scss'",
    "lint-css:blocks:fix": "npm run lint-css:blocks -- --fix",
    "lint-css-fix": "npm run lint-css:src:fix && npm run lint-css:blocks:fix",
    "make-pot": "wp i18n make-pot --domain=\"ystandard-blocks\" --exclude=\"library\" ./ languages/ystandard-blocks.pot",
    "po2json:blocks": "po2json languages/ystandard-blocks-ja.po languages/ystandard-blocks-ja-ystandard-blocks-editor.json -f jed -p",
    "build:block": "wp-scripts build --config webpack.blocks.config.js",
    "build:icons": "node src/js/script/create-svg-icon-list.js",
    "build:admin": "wp-scripts build --config webpack.admin-menu.config.js && gulp copyAdminMenuFiles",
    "build:css": "gulp sass",
    "build": "npm run build:block && npm run build:admin && npm run build:css",
    "watch:block": "wp-scripts start --config webpack.blocks.config.js",
    "watch:sass": "gulp watchSass",
    "watch:admin-menu": "NODE_ENV=development wp-scripts start --config webpack.admin-menu.config.js & gulp watchAdminMenu",
    "watch:admin": "npm run watch:admin-menu  & npm run watch:sass",
    "watch": "npm run watch:block & gulp watch",
    "zip": "gulp zip",
    "build:deploy-files": "gulp createDeployFiles",
    "build:deploy-files-beta": "gulp createDeployFilesBeta",
    "packages-update": "wp-scripts packages-update",
    "production": "npm run build && npm run build:deploy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yosiakatsuki/ystandard-blocks.git"
  },
  "author": "",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/yosiakatsuki/ystandard-blocks/issues"
  },
  "homepage": "https://github.com/yosiakatsuki/ystandard-blocks#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@wordpress/browserslist-config": "^4.1.0",
    "@wordpress/icons": "^5.0.1",
    "@wordpress/scripts": "^17.1.0",
    "autoprefixer": "^10.3.2",
    "browserslist": "^4.15.0",
    "classnames": "^2.3.1",
    "css-declaration-sorter": "^6.1.1",
    "css-loader": "^3.6.0",
    "cssnano": "^5.0.8",
    "deepmerge": "^4.2.2",
    "del": "^6.0.0",
    "feather-icons": "^4.28.0",
    "fibers": "^5.0.0",
    "file-loader": "^4.3.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-zip": "^5.1.0",
    "lodash": "^4.17.20",
    "po2json": "^1.0.0-beta-3",
    "prettier": "^2.3.2",
    "react-feather": "^2.0.9",
    "react-toastify": "^8.0.0",
    "sass": "^1.38.1",
    "sass-loader": "^8.0.2",
    "simple-icons": "^5.11.0",
    "style-loader": "^1.3.0",
    "webpack": "^4.46.0",
    "webpack-stream": "^6.1.2"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
