{
  "name": "ystandard-blocks",
  "version": "0.0.1",
  "description": "yStandard用ブロック追加プラグイン",
  "scripts": {
    "lint-all": "npm run lint-js:fix && npm run lint-css-fix",
    "lint-js": "wp-scripts lint-js blocks ./src/js",
    "lint-js:fix": "npm run lint-js -- --fix",
    "lint-css:src": "wp-scripts lint-style 'src/**/*.scss'",
    "lint-css:src:fix": "npm run lint-css:src -- --fix",
    "lint-css:blocks": "wp-scripts lint-style 'blocks/**/*.scss'",
    "lint-css:blocks:fix": "npm run lint-css:blocks -- --fix",
    "lint-css-fix": "npm run lint-css:src:fix && npm run lint-css:blocks:fix",
    "start:block": "wp-scripts start --config webpack.blocks.config.js",
    "build:block": "wp-scripts build --config webpack.blocks.config.js",
    "build:block-app": "gulp js:block-app",
    "build:icon": "node src/js/util/create-icon-list.js",
    "build:svg-icon": "node src/js/util/create-svg-icon-list.js",
    "build:css": "gulp sass",
    "build": "npm run build:block && gulp sass && gulp webpack",
    "watch": "npm run start:block & gulp watch",
    "zip": "gulp zip",
    "build:deploy-files": "gulp createDeployFiles",
    "build:deploy-files-beta": "gulp createDeployFilesBeta",
    "packages-update": "wp-scripts packages-update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yosiakatsuki/ystandard-blocks.git"
  },
  "author": "",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/yosiakatsuki/ystandard-blocks/issues"
  },
  "homepage": "https://github.com/yosiakatsuki/ystandard-blocks#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@wordpress/browserslist-config": "^2.7.0",
    "@wordpress/icons": "^1.4.0",
    "@wordpress/scripts": "12.5.0",
    "autoprefixer": "^9.8.6",
    "browserslist": "^4.15.0",
    "classnames": "^2.2.6",
    "css-declaration-sorter": "^5.1.2",
    "css-loader": "^3.6.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "deepmerge": "^4.2.2",
    "del": "^5.1.0",
    "feather-icons": "^4.28.0",
    "fibers": "^5.0.0",
    "file-loader": "^4.3.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-zip": "^5.0.2",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "react-feather": "^2.0.9",
    "sass": "^1.30.0",
    "sass-loader": "^8.0.2",
    "simple-icons": "^2.19.0",
    "style-loader": "^1.3.0",
    "uikit": "^3.5.12",
    "vue": "^2.6.12",
    "vue-color": "^2.7.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.3.19",
    "watch": "^1.0.2",
    "webpack": "^4.44.2",
    "webpack-stream": "^5.2.1"
  },
  "dependencies": {
    "@fonticonpicker/react-fonticonpicker": "^1.2.0",
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.4.1"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ]
}
